  ],
  "bugs": {
    "url": "https://github.com/JB1905/where-is-my-node/issues"
  },
  "homepage": "https://github.com/JB1905/where-is-my-node#readme",
  "bin": {
    "where-is-my-node": "bin/index.js",
    "my-node": "bin/index.js",
    "wimn": "bin/index.js"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "figlet": "^1.2.4",
    "node-fetch": "^2.6.0",
    "npm-path": "^2.0.4"
  },
  "devDependencies": {
    "@types/figlet": "^1.2.0",
    "@types/node": "^13.1.8",
    "@types/node-fetch": "^2.5.4",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
